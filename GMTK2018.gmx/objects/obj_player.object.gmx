            <string>///Super HOT Grapple Physics and Gravity (So HOT)

if mouse_check_button_pressed(mb_left){
    if !(instance_exists(obj_grappleHead)){
            instance_create(x, y, obj_grappleHead)
        }
}

if instance_exists(obj_grappleHead){
    if obj_grappleHead.stuck{
        dx = obj_grappleHead.x - x;
        dy = obj_grappleHead.y - y;
        
        len = sqrt(dx*dx + dy*dy);
        
        dx = (dx/len);
        dy = (dy/len);
        
        vx = vx + dx * grapspeed;
        vy = vy + dy * grapspeed;
    }
}


//For Collisions
if place_meeting(x, y, obj_grappleHead) and obj_grappleHead.stuck{
    vx = 0;
    vy = 0;
}
if place_meeting(x + sprite_width/2, y, obj_wall){
    if !walled{
        vx = 0;
        walled = true;
    }
    if vx &gt; 0{
        vx = 0;
    }
} else if place_meeting(x - sprite_width/2, y, obj_wall){
    if !walled{
        vx = 0;
        walled = true;
    }
    if vx &lt; 0{
        vx = 0;
    }
} else{
    walled = false;
}

if place_meeting(x, y + sprite_height/2, obj_wall){
    if !floored{
        vy = 0;
        floored = true;
    }
    if vy &gt; 0{
        vy = 0;
    }
} else if place_meeting(x, y - sprite_height/2, obj_wall){
    if !floored {
        vy = 0.5;
        floored = true;
    }
    if vy &lt; 0{
        vy = 0;
    }
} else{
    floored = false;
}


//Moves the chara
x += vx;
y += vy;

vy += 1;
if(vx != 0){
    vx -= (vx/abs(vx))*0.05;
}


vx = clamp(vx, -20, 20);
vy = clamp(vy, -20, 20);
</string>
